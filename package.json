    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint-plugin-react": "^5.1.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "cosmicjs": "^2.1.2",
    "express": "^4.14.0",
    "hogan-express": "^0.5.2",
    "mobx": "^2.2.2",
    "mobx-react": "^3.3.1",
    "mobx-react-devtools": "^4.2.0",
    "osmosis": "^1.1.4",
    "react": "^15.1.0",
